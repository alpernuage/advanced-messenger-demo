{
    "name": "alp/advanced-messenger-demo",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=8.3",
        "doctrine/doctrine-bundle": "^2.11",
        "doctrine/orm": "^3.0",
        "symfony/console": "^7.0",
        "symfony/doctrine-messenger": "^7.0",
        "symfony/dotenv": "^7.0",
        "symfony/flex": "^2.4",
        "symfony/framework-bundle": "^7.0",
        "symfony/messenger": "^7.0",
        "symfony/redis-messenger": "^7.0",
        "symfony/runtime": "^7.0",
        "symfony/validator": "^7.3",
        "symfony/yaml": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "symfony/phpunit-bridge": "^7.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.10",
        "rector/rector": "^0.18"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
